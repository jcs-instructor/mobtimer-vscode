{
  "name": "mobtimer-vscodeversion20",
  "displayName": "Mobtimer for VSCode",
  "repository": "mobtimer-vscode",
  "description": "",
  "version": "0.0.20",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "mobtimer.helloWorldversion20"
  ],
  "main": "./dist/extension.js",
